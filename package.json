{
  "name": "rubiks-cube-viewer",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "Interactive Rubik's Cube solver using Three.js and Kociemba algorithm",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "format": "biome format --write",
    "generate:solver-tables": "node scripts/generate-solver-tables.js"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@playwright/test": "^1.57.0",
    "@vitest/browser": "^1.0.4",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "happy-dom": "^12.10.3",
    "http-server": "^14.1.1",
    "playwright": "^1.40.1",
    "vite": "^7.3.1",
    "vitest": "^1.0.4"
  },
  "dependencies": {
    "cubejs": "^1.3.2",
    "three": "^0.160.1"
  }
}
